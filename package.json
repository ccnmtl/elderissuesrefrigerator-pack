    "css-loader": "^6.2.0",
    "eslint": "~8.13.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^6.0.0",
    "loglevel": "^1.4.1",
    "mocha": "^9.0.0",
    "mocha-chrome": "^2.2.0",
