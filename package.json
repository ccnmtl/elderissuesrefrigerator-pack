{
  "name": "elderissuesrefrigerator-pack",
  "version": "0.1.10",
  "description": "Interactive to help understand issues faced by older adults",
  "main": "src/index.js",
  "scripts": {
    "serve": "webpack-dev-server --hot --inline --open",
    "test": "npm run test-unit && npm run test-client",
    "test-unit": "mocha test/model-test.js",
    "test-client": "./node_modules/phantomjs-prebuilt/bin/phantomjs ./node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js dist/view-test.html",
    "build": "webpack && webpack --config test/test.webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ccnmtl/elderissuesrefrigerator-pack.git"
  },
  "keywords": [
    "webpack",
    "phantomjs",
    "mocha",
    "chai",
    "phantomjs",
    "jquery",
    "client",
    "ctl"
  ],
  "author": {
    "name": "Zarina Mustapha",
    "email": "zarina@columbia.edu"
  },
  "bugs": {
    "url": "https://github.com/ccnmtl/elderissuesrefrigerator-pack/issues"
  },
  "license": "GPL-3.0",
  "greenkeeper": {
    "ignore": [
      "bootstrap",
      "webpack"
    ]
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "eslint": "~5.0.0",
    "eslint-config-scanjs": "^1.0.0-beta4",
    "eslint-plugin-no-unsafe-innerhtml": "^1.0.16",
    "eslint-plugin-security": "^1.4.0",
    "loglevel": "^1.4.1",
    "mocha": "^5.0.0",
    "mocha-phantomjs-core": "^2.1.2",
    "underscore-template-loader": "^1.0.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "style-loader": "^0.21.0",
    "phantomjs-prebuilt": "^2.1.15",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {
    "backbone": "1.3.3",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "underscore": "1.9.1",
    "webpack": "^4.6.0",
    "webpack-cli": "~3.1.0"
  }
}
